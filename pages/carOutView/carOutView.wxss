/* pages/carOutView/carOutView.wxss */
@import "../common/lib/weui.wxss";

.yuan-icon {
  width: 30rpx;
  height: 30rpx;
  border: 1rpx #b9b9b9 solid;
  border-radius: 50%;
  margin-right: 6rpx;
}

.active {
  background: #50cc98;
  border: none;
}

.activev {
  color: #50cc98;
}

.iconimage {
  width: 84rpx;
  height: 60rpx;
}

.table {
  width: 100%;
  background: #fff;
}

.tr {
  width: 100%;
  display: flex;
  justify-content: space-between;
}

.th, .td {
  font-size: 30rpx;
  padding: 20rpx;
  text-align: center;
  width: 100%;
}

.th {
  font-weight: bolder;
}

.td {
  color: #aeaeae;
}

.buttonBox {
  display: -webkit-flex;
  position: absolute;
  left: 0;
  bottom: 20px;
  width: 100%;
  background: #fff;
}
